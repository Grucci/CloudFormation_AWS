{
    "if": {
        "allOf": [
            {
                "field": "type",
                "equals": "Microsoft.Web/serverfarms"
            },
            {
                "not": {
                    "field": "Microsoft.Web/serverfarms/zoneRedundant",
                    "equals": true
                }
            }
        ]
    },
    "then": {
        "effect": "[parameters('effect')]"
    }
}